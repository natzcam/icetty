{
  "name": "icetty",
  "description": "secure p2p command line access to any of your devices",
  "version": "0.0.4",
  "author": "Nathaniel Camomot",
  "bin": {
    "icetty": "./bin/run"
  },
  "bugs": "https://github.com/natzcam/icetty/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.8.8",
    "@oclif/plugin-help": "^2.1.3",
    "conf": "^2.0.0",
    "conf-cli": "^0.1.6",
    "debug": "^4.1.0",
    "firebase": "^5.5.6",
    "firepeer": "^0.1.6",
    "get-port": "^4.0.0",
    "google-auth-library": "^2.0.0",
    "google-device-auth": "^0.1.3",
    "hybrid-crypto-js": "^0.1.6",
    "inquirer": "^6.2.0",
    "keypress": "^0.2.1",
    "node-notifier": "^5.3.0",
    "node-pty": "^0.7.8",
    "opn": "^5.4.0",
    "tslib": "^1.9.3",
    "wrtc": "^0.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.0",
    "@oclif/test": "^1.2.1",
    "@oclif/tslint": "^3.1.0",
    "@types/chai": "^4.1.6",
    "@types/conf": "^1.4.0",
    "@types/debug": "0.0.31",
    "@types/get-port": "^4.0.0",
    "@types/inquirer": "0.0.43",
    "@types/mocha": "^5.2.5",
    "@types/nanoid": "^1.2.0",
    "@types/node": "^10.12.0",
    "@types/node-notifier": "0.0.28",
    "@types/opn": "^5.1.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json",
    ".keys"
  ],
  "homepage": "https://github.com/natzcam/icetty",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "icetty",
    "plugins": [
      "@oclif/plugin-help",
      "conf-cli"
    ]
  },
  "repository": "natzcam/icetty",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "login": "tsc && cross-env DEBUG=icetty icetty login",
    "start": "tsc && cross-env DEBUG=icetty icetty start",
    "connect": "tsc && cross-env DEBUG=icetty icetty connect"
  },
  "types": "lib/index.d.ts"
}
